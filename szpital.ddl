CREATE OR REPLACE TRIGGER trigger_cho BEFORE INSERT OR UPDATE ON choroby FOR EACH ROW 
    DECLARE vKomunikat VARCHAR(50);
    vMax NUMBER;
    exceptionWrongData EXCEPTION;
    PRAGMA EXCEPTION_INIT (exceptionWrongData, -1);
BEGIN
    vKomunikat:= null;
    select max(id_oddzialu) into vMax from oddzialy; 


    if :NEW.nazwa is null then vKomunikat:= 'Podaj nazwe choroby';
    elsif :NEW.id_choroby is null then :NEW.id_choroby:=id_choroba.nextval;
    elsif :NEW.id_spec not between 1 and vMax then vKomunikat:= 'Podaj jedną ze specjalizacji';
    end if;

    if vKomunikat is not null then 
        DBMS_OUTPUT.PUT_LINE('ERROR-001\: '||vKomunikat);
        raise exceptionWrongData;       
    end if;
END;

