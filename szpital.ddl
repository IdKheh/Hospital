-------------------------------------- FUNKCJE -----------------------------------------------------
CREATE FUNCTION WolneMiejscaNaOddziale (pIdOddzialu IN NUMBER) RETURN NATURAL IS vWolneLozka NATURAL;
BEGIN
    SELECT COUNT(*) INTO vWolneLozka FROM sale WHERE id_oddzialu = pIdOddzialu;
    RETURN vWolneLozka;
END WolneMiejscaNaOddziale;

CREATE OR REPLACE FUNCTION pacjentDoSali (vPesel CHAR) RETURN NATURAL IS 
    vSala NATURAL := 0;
    vChoroba Number :=0;
    vOddzial Number := 0;
    vWolne Number := 0;
    exceptionWrongData EXCEPTION;
    PRAGMA EXCEPTION_INIT (exceptionWrongData, -1);
BEGIN
    SELECT ID_CHOROBY INTO vChoroba FROM diagnoza WHERE pesel = vPesel order by data_diagnozy desc FETCH FIRST 1 ROWS ONLY;
    SELECT id_spec INTO vOddzial FROM choroby WHERE id_choroby = vChoroba;
    vWolne:= WolneMiejscaNaOddziale(vOddzial);

    if vWolne>0 then 
        --szukam chorych na tą samą chorobę i są razem w sali jesli są wolne łóżka
        FOR vNr_sal IN (select sale_nr_sali from diagnoza inner join sale_pacjenci on sale_pacjenci.pacjenci_pesel=diagnoza.pesel 
        where ID_CHOROBY= vChoroba order by data_diagnozy desc) LOOP
            select nr_sali into vSala from sale where vNr_sal.sale_nr_sali = nr_sali and id_oddzialu = vChoroba and wolne_lozka > 0 FETCH FIRST 1 ROWS ONLY;
        END LOOP;

        if vSala = 0 then select nr_sali into vSala from sale where id_oddzialu = vChoroba and wolne_lozka = LOZKA FETCH FIRST 1 ROWS ONLY; -- szukamy pustej sali by nie zarazal innych
        end if;
        if vSala = 0 then select nr_sali into vSala from sale where id_oddzialu = vChoroba and wolne_lozka > 0 FETCH FIRST 1 ROWS ONLY; -- niestety mieszamy pacjentów
        end if;
    else DBMS_OUTPUT.PUT_LINE('ERROR-001\: Brak wolnych miejsc na oddziale');
        raise exceptionWrongData;
    end if;

    RETURN vSala;
END pacjentDoSali;

-------------------------------------- PROCEDURY -----------------------------------------------------
CREATE OR REPLACE PROCEDURE ZnajdzPacjentowLekarza (pIdLekarza IN NUMBER) IS
BEGIN
    FOR vPacjent IN (SELECT * FROM diagnoza WHERE id_lekarza = pIdLekarza) LOOP
        DBMS_OUTPUT.PUT_LINE(vPacjent.Pesel||' '|| vPacjent.id_lekarza||' '|| vPacjent.id_choroby||' '||vPacjent.data_diagnozy||' '||vPacjent.id_wizyty);
    END LOOP;
END ZnajdzPacjentowLekarza;

CREATE OR REPLACE PROCEDURE HistoriaChorobyPacjentaPesel (vPesel CHAR) IS
BEGIN
    FOR vWizyta IN (
        select pacjenci.pesel,pacjenci.imie, pacjenci.nazwisko, choroby.nazwa as choroba, PERSONEL_MEDYCZNY.NAZWISKO as lekarz,wizyty.DATA_PRZYJECIA,
        wizyty.DATA_WYPISU, diagnoza.DATA_DIAGNOZY from wizyty inner join diagnoza on wizyty.ID_WIZYTY=diagnoza.id_wizyty inner join PERSONEL_MEDYCZNY 
        on ID_MEDYCY = id_lekarza  inner join choroby on choroby.ID_CHOROBY = diagnoza.id_choroby inner join PACJENCI on pacjenci_pesel = PACJENCI.PESEL 
        where PACJENCI_PESEL = vPesel ) LOOP
        DBMS_OUTPUT.PUT_LINE(vWizyta.pesel||' '|| vWizyta.imie||' '|| vWizyta.nazwisko||' '||vWizyta.choroba||' '||vWizyta.lekarz||' '||vWizyta.DATA_PRZYJECIA||' '||vWizyta.DATA_WYPISU||' '||vWizyta.DATA_DIAGNOZY);
    END LOOP;
END HistoriaChorobyPacjentaPesel;

CREATE OR REPLACE PROCEDURE HistoriaChorobyPacjentaImieNazwisko (vImie in VARCHAR2, vNazwisko in VARCHAR2) IS
BEGIN
    FOR vWizyta IN (
        select pacjenci.pesel,pacjenci.imie, pacjenci.nazwisko, choroby.nazwa as choroba, PERSONEL_MEDYCZNY.NAZWISKO as lekarz,wizyty.DATA_PRZYJECIA,
        wizyty.DATA_WYPISU, diagnoza.DATA_DIAGNOZY from wizyty inner join diagnoza on wizyty.ID_WIZYTY=diagnoza.id_wizyty inner join PERSONEL_MEDYCZNY 
        on ID_MEDYCY = id_lekarza  inner join choroby on choroby.ID_CHOROBY = diagnoza.id_choroby inner join PACJENCI on pacjenci_pesel = PACJENCI.PESEL 
        where pacjenci.imie like vImie and pacjenci.nazwisko like vNazwisko) LOOP
        DBMS_OUTPUT.PUT_LINE(vWizyta.pesel||' '|| vWizyta.imie||' '|| vWizyta.nazwisko||' '||vWizyta.choroba||' '||vWizyta.lekarz||' '||vWizyta.DATA_PRZYJECIA||' '||vWizyta.DATA_WYPISU||' '||vWizyta.DATA_DIAGNOZY);
    END LOOP;
END HistoriaChorobyPacjentaImieNazwisko;

-------------------------------------- TRIGGERY -----------------------------------------------------
CREATE OR REPLACE TRIGGER trigger_med BEFORE INSERT OR UPDATE ON personel_medyczny FOR EACH ROW 
    DECLARE vKomunikat VARCHAR(50);
    exceptionWrongData EXCEPTION;
    PRAGMA EXCEPTION_INIT (exceptionWrongData, -1);
BEGIN
    vKomunikat:= null;
    
    if :NEW.imie is null then vKomunikat:= 'Podaj imie';
    elsif :new.id_medycy IS NULL then :NEW.id_medycy:=id_medyka.nextval;
    elsif :NEW.nazwisko is null then vKomunikat:= 'Podaj nazwisko';
    elsif not REGEXP_LIKE(:NEW.nr_telefonu, '^\d{9}$') then vKomunikat:= 'Podaj poprawny polski numer telefonu';
    elsif :NEW.typ NOT IN ('lekarz', 'pielegniarka') then vKomunikat:= 'Podaj poprawny typ (lekarz, pielegniarka)';
    end if;

    if vKomunikat is not null then 
        DBMS_OUTPUT.PUT_LINE('ERROR-001\: '||vKomunikat);
        raise exceptionWrongData;
    elsif vKomunikat is null and :new.typ = 'lekarz' then INSERT into lekarze (id_medycy) VALUES (:new.id_medycy);
    elsif vKomunikat is null and :new.typ = 'pielegniarka' then INSERT into lekarze (id_medycy) VALUES (:new.id_medycy);
    end if;
END;

